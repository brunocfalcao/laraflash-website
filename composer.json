{
    "name": "brunocfalcao/laraflash-website",
    "description": "Laraflash - Your Laravel flash news!",
    "type": "library",
    "keywords": [
        "laraflash",
        "laravel",
        "brunocfalcao"
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "~5.8.0",
        "coconutcraig/laravel-postmark": "^2.2",
        "brunocfalcao/flame": "dev-master",
        "brunocfalcao/laraflash-dal": "dev-master",
        "brunofalcao/helpers": "dev-master",
        "pragmarx/firewall": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laraflash\\Website\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Falcao",
            "email": "bruno.c.falcao@brunofalcao.me"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Laraflash\\Website\\Providers\\BaseServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
